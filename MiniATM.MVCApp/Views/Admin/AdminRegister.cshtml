<form action="/admin/register" method="post"> 
    <div class="container-fluid">
        <div class=" p-5">
            <article>
                <div class="mb-3">
                    <label class="form-label fw-bold">AdminUsername</label>
                    <input type="text" name="AdminUsername" class="form-control w-full">
                </div>
                <div>
                    <label class="form-label fw-bold">AdminPassword</label>
                    <input type="text" name="AdminPassword" class="form-control w-full">
                </div>
            </article>
        </div>
        <div class=" ms-5 pb-3">
            <a href="/admin/list" type="button" class="btn btn-secondary">Cancel</a>
            <button type="submit" class="btn btn-success">Save</button>
        </div>
    </div>
</form>
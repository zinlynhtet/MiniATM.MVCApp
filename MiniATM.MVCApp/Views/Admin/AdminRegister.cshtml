<form action="/admin/register" method="post"> 
	
    <div class=" container-fluid shadow bg-dark ">
        <div class=" p-5 d-flex gap-5">
            <article>
                <div class="mb-3">
                    <label class="form-label text-white-50 fw-bold">AdminUsername</label>
                    <input type="text" name="AdminUsername" class="form-control w-full">
                </div>
                <div class="mb-3">
                    <label class="form-label text-white-50 fw-bold">AdminPassword</label>
                    <input type="text" name="AdminPassword" class="form-control w-full">
                </div>
            </article>
        </div>
        <div class="d-flex justify-content-center gap-3 pb-3">
            <a href="/admin" type="button" class="btn btn-secondary">Cancel</a>
            <button type="submit" class="btn btn-success">Save</button>
        </div>
    </div>
	
</form>
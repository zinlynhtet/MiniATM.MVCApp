    <aside class="app-navbar">
        <!-- begin sidebar-nav -->
        <div class="sidebar-nav scrollbar scroll_light">
            <ul class="metismenu " id="sidebarNav">
                <li class="nav-static-title">Personal</li>
                @{
                    var httpContextAccessor = new HttpContextAccessor();
                    var menuList = JsonConvert.DeserializeObject<List<MenuModel>>(httpContextAccessor.HttpContext.Session.GetString("Menu"));
                }
                @foreach (var item in menuList)
                {
                    <li>
                        @{
                            var route = string.IsNullOrWhiteSpace(item.ManinMenuController) ? "javascript:void(0)"
                            : $"/{item.ManinMenuController}/{item.ActionName}";
                        }
                        <a class="has-arrow" href="@route" aria-expanded="false"><span class="nav-title">@item.MainMenu</span></a>
                        <ul aria-expanded="false">
                            @foreach (var subMenu in item.SubMenus)
                            {
                                <li> <a href="/@subMenu.SubMenuController/@subMenu.ActionName">@subMenu.SubMenu</a></li>
                            }
                        </ul>
                    </li>
                }

            </ul>
        </div>
        <!-- end sidebar-nav -->
    </aside>
    <div class="app-main" id="main">
        <!-- begin container-fluid -->
